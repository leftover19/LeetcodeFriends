{
  "manifest_version": 2,
  "name": "Leetcode Friend's saver",
  "version": "1.0",
  "description": "Saves username on Leetcode and provides the number of questions solved",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "icons/leetcode.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdnjs.cloudflare.com/ https://ajax.googleapis.com/; object-src 'self'",
  "background": {
    "scripts": ["background.js"]
  }
}
